<?xml version="1.0" ?>
<sdf version="1.7">
  <world name="test_move_world">
    <gui>
      <!-- Load the Editing (transform) system plugin -->
      <plugin filename="libignition-gazebo-editing-system.so"
              name="ignition::gazebo::systems::Editing">
        <ignition-gui>
          <property type="bool" key="showTitleBar">false</property>
          <property type="string" key="state">docked</property>
        </ignition-gui>
      </plugin>
    </gui>

    <!-- A single movable box -->
    <model name="movable_box">
      <static>false</static>
      <link name="link">
        <pose>0 0 0.5 0 0 0</pose>
        <collision name="coll">
          <geometry>
            <box><size>1 1 1</size></box>
          </geometry>
        </collision>
        <visual name="vis">
          <geometry>
            <box><size>1 1 1</size></box>
          </geometry>
        </visual>
      </link>
    </model>
  </world>
</sdf>

